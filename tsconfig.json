{
    "compilerOptions": {
        "noImplicitAny": true,
        "allowJs": false,
        "moduleResolution":"node",
        "removeComments": false,
        "target": "ES2020",
        "declaration": false,
        "declarationMap": false,
        "inlineSourceMap": false,
        "inlineSources": false,
        "strict": true,
        "esModuleInterop": true,
        "sourceMap": false,
        "preserveConstEnums": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noEmit": false,
    },
    
    "exclude": [
        "node_modules"
    ],
    
    "include": ["./"]
  
  }