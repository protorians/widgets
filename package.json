{
  "name": "@protorians/widgets",
  "version": "0.0.4",
  "description": "Create your web user interfaces with widgets",
  "main": "./~esm/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch -p esm.tsconfig.json",
    "build": "pnpm run build:esm",
    "build:esm": "tsc -p esm.tsconfig.json",
    "build:cjs": "tsc -p cjs.tsconfig.json",
    "build:types": "npm-dts generate -o ./globals.d.ts -c",
    "postinstall": "tsc -p esm.tsconfig.json"
  },
  "exports": {
    "import": "./~esm/index.js",
    "default": "./index.ts"
  },
  "keywords": [
    "protorians",
    "widget",
    "library"
  ],
  "author": "Y. Yannick GOBOU",
  "repository": {
    "url": "https://github.com/protorians/widgets.git"
  },
  "license": "ISC",
  "dependencies": {
    "@protorians/signalable": "^0.0.10",
    "typescript": "^5.4.5"
  }
}
